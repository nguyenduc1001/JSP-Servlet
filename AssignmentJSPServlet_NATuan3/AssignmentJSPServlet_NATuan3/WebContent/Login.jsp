		<script>
				$("#btn-login").click(function(){
					var email = $("#input-name").val();
					var pwd = $("#txt-password").val();
					$.ajax({
						url : "MainServlet",
						type : "GET",
						data : {
							email : email,
							pwd : pwd
						},
						success : function(value) {
							 if (value.toString() == "true") {
								alert("Đăng nhập thành công");
								window.location.assign("HomeServlet");
							} else {
								alert("Đăng nhập tài khoản thất bại");
							}
						}
					});
					
				});
